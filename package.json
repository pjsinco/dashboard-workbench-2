{
  "name": "dashboard-workbench-2",
  "description": "Prototyping the member dashboard",
  "version": "1.0.0",
  "author": "pjsinco <psinco@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "prepublish": "rollup -c && ./node_modules/.bin/uglifyjs dist/d3.js -c -m -o dist/d3.min.js",
    "unit": "clear; jest"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.5.2",
    "flexboxgrid-sass": "^8.0.5",
    "vue": "^2.3.3",
    "vue-router": "^2.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-2": "^6.24.1",
    "countup.js": "^1.9.2",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "d3-ease": "^1.0.3",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-timer": "^1.0.7",
    "d3-transition": "^1.1.0",
    "file-loader": "^0.9.0",
    "jest": "^21.1.0",
    "jest-serializer-vue": "^0.2.0",
    "jest-vue": "^0.5.6",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "sass-loader": "^5.0.1",
    "uglify-js": "^3.1.2",
    "vue-loader": "^12.1.0",
    "vue-server-renderer": "^2.4.2",
    "vue-template-compiler": "^2.3.3",
    "vue-test-utils": "git+https://github.com/vuejs/vue-test-utils.git",
    "vuex": "^2.4.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "moduleNameMapper": {
      "d3": "<rootDir>/dist/d3.min.js",
      "countUp": "<rootDir>/node_modules/countup.js/countUp.js"
    }
  }
}
